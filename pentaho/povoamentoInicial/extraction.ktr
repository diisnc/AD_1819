    <sql>SELECT CAST(CONCAT(YEAR(order_date),MONTH(order_date),DAY(order_date)) AS UNSIGNED),
CONCAT(YEAR(order_date), "-", MONTH(order_date), "-", DAY(order_Date)), 
WEEKDAY(order_date), DAY(order_date), MONTH(order_date), YEAR(order_date), QUARTER(order_date), FLOOR((QUARTER(order_date)/3)+1)
FROM northwind.orders WHERE order_date IS NOT NULL

UNION 

SELECT CAST(CONCAT(YEAR(shipped_date),MONTH(shipped_date),DAY(shipped_date)) AS UNSIGNED),
CONCAT(YEAR(shipped_date), "-", MONTH(shipped_date), "-", DAY(shipped_Date)), 
WEEKDAY(shipped_date), DAY(shipped_date), MONTH(shipped_date), YEAR(shipped_date), QUARTER(shipped_date), FLOOR((QUARTER(shipped_date)/3)+1)
FROM northwind.orders WHERE shipped_date IS NOT NULL

UNION 

SELECT CAST(CONCAT(YEAR(paid_date),MONTH(paid_date),DAY(paid_date)) AS UNSIGNED),
CONCAT(YEAR(paid_date), "-", MONTH(paid_date), "-", DAY(paid_Date)), 
WEEKDAY(paid_date), DAY(paid_date), MONTH(paid_date), YEAR(paid_date), QUARTER(paid_date), FLOOR((QUARTER(paid_date)/3)+1)
FROM northwind.orders WHERE paid_date IS NOT NULL;</sql>
